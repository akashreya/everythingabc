{
  "name": "everythingabc-api",
  "version": "1.0.0",
  "description": "EverythingABC Visual Vocabulary Platform API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "seed": "node scripts/seed.js",
    "migrate": "node scripts/migrate.js",
    "export": "node scripts/export-categories.js",
    "import": "node scripts/import-categories.js",
    "migrate:images": "node scripts/migration/01-migrate-images.js",
    "migrate:items": "node scripts/migration/02-migrate-items.js",
    "migrate:images:dry": "set DRY_RUN=true && node scripts/migration/01-migrate-images.js",
    "migrate:items:dry": "set DRY_RUN=true && node scripts/migration/02-migrate-items.js",
    "migrate:rollback": "node scripts/migration/01-migrate-images.js --rollback && node scripts/migration/02-migrate-items.js --rollback",
    "test:v2": "node scripts/test-v2-urls.js",
    "test:v2:local": "node scripts/test-v2-urls.js http://localhost:3003",
    "test:v2:bash": "bash scripts/test-v2-urls.sh",
    "test:v2:regression": "node scripts/test-v2-urls.js",
    "test:v2:quick": "node scripts/test-v2-quick.js",
    "db:setup:test": "node scripts/setup-test-db.js test",
    "db:setup:dev": "node scripts/setup-test-db.js development",
    "db:setup:local": "node scripts/setup-test-db.js local",
    "env:switch": "node scripts/env-switch.js",
    "env:dev": "node scripts/env-switch.js development",
    "env:test": "node scripts/env-switch.js test",
    "env:local": "node scripts/env-switch.js local",
    "env:current": "node scripts/env-switch.js current",
    "dev:memory": "npm run env:dev && npm run db:setup:dev && npm run dev",
    "test:setup": "npm run env:test && npm run db:setup:test && npm test"
  },
  "keywords": [
    "vocabulary",
    "learning",
    "education",
    "api"
  ],
  "author": "EverythingABC Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1400.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^5.8.2",
    "sharp": "^0.34.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
